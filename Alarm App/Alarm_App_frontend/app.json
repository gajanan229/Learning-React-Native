{
  "expo": {
    "name": "ChronoFold",
    "slug": "chronofold",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "export",
      "favicon": "./assets/images/favicon.png",
      "jsEngine": "jsc"
    },
    "plugins": ["expo-router", "expo-font", "expo-web-browser", "expo-notifications"],
    "experiments": {
      "typedRoutes": true
    }
  }
}